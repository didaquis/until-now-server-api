PORT=5000

# Database format connection (choose between: "standard" or "DNSseedlist"):
MONGO_FORMAT_CONNECTION=standard

# Example of localhost standard database connection:
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_DB=database
MONGO_USER=
MONGO_PASS=

# Example of cloud database connection using DNSseedlist:
MONGO_DNS_SEEDLIST_CONNECTION=mongodb+srv://<user>:<password>@<uri-and-options>

# AUTH CONFIG:
JWT_SECRET=your-secret
JWT_EXP=3600